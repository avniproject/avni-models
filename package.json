{
  "name": "openchs-models",
  "description": "OpenCHS data model to be used by front end clients",
  "version": "0.0.3",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenCHS/openchs-client.git"
  },
  "main": "lib/openchs-models.js",
  "scripts": {
    "test": "jest",
    "test-ci": "jest --verbose --forceExit --maxWorkers=2 --ci",
    "coverage": "istanbul cover --report lcov _mocha --require $(find ./src/test -name '*Test.js')",
    "build": "webpack --env dev && webpack -p --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/test/**/*Test.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|OpenCHSClient)/"
    ]
  },
  "license": "AGPL-3.0",
  "dependencies": {
    "lodash": "^4.17.11",
    "moment": "^2.22.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-register": "^7.0.0-beta.3",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "jest": "^24.8.0",
    "jsdom": "15.1.1",
    "jsdom-global": "3.0.2",
    "nyc": "^14.1.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.12.2",
    "webpack-cli": "^3.0.8",
    "yargs": "^13.2.4"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  }
}
